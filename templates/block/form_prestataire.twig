        <input type="text" id="nom_pre" name="nom" class="form-control" aria-label="Sizing example input" aria-describedby="lb_nom_pre" required>
    </div>
    <div class="input-group mb-3">
        <span class="input-group-text" id="lb_tva_pre">tva</span>
